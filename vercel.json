{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index.php"
    },
    {
      "source": "/about",
      "destination": "/api/about.php"
    },
    {
      "source": "/api/users",
      "destination": "/api/users.php"
    }
  ]
}